<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>

	<General>
	    <ROOT>
		<TCEforms>
		    <sheetTitle>LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xml:display_flexform.sheet_general</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
		    <settings.suggestions>
			<TCEforms>
			    <label>LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xml:session_flexform.settings.suggestions</label>
			    <config>
				<type>check</type>
			    </config>
			</TCEforms>
		    </settings.suggestions>
		    <settings.listViewHeadline>
			<TCEforms>
			    <label>LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xml:session_flexform.settings.listViewHeadline</label>
			    <config>
				<max>256</max>
				<size>50</size>
				<softref>typolink_tag</softref>
				<type>input</type>
			    </config>
			</TCEforms>
		    </settings.listViewHeadline>
		    <settings.listViewText>
			<TCEforms>
			    <label>LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xml:session_flexform.settings.listViewText</label>
			    <config>
				<cols>80</cols>
				<rows>15</rows>
				<softref>rtehtmlarea_images,typolink_tag,images,email[subst],url</softref>
				<type>text</type>
				<wizards>
				    <RTE>
					<icon>wizard_rte2.gif</icon>
					<notNewRecords>1</notNewRecords>
					<RTEonly>1</RTEonly>
                    <module>
                        <name>wizard_rte</name>
                    </module>
					<title>LLL:EXT:cms/locallang_ttc.xml:bodytext.W.RTE</title>
				    </RTE>
				</wizards>
			    </config>
			    <defaultExtras>richtext:rte_transform[flag=rte_enabled|mode=ts_css]</defaultExtras>
			</TCEforms>
		    </settings.listViewText>
		    <settings.days>
			<TCEforms>
			    <label>LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xml:session_flexform.settings.days</label>
			    <config>
				<autoSizeMax>100</autoSizeMax>
				<allowed>tx_sessionplaner_domain_model_day</allowed>
				<internal_type>db</internal_type>
				<maxitems>100</maxitems>
				<minitems>0</minitems>
				<size>2</size>
				<type>group</type>
				<wizards>
				    <suggest>
					<type>suggest</type>
				    </suggest>
				</wizards>
			    </config>
			</TCEforms>
		    </settings.days>
		    <settings.singlePid>
			<TCEforms>
			    <label>LLL:EXT:sessionplaner/Resources/Private/Language/locallang_be.xml:session_flexform.settings.singlePid</label>
			    <config>
				<allowed>pages</allowed>
				<internal_type>db</internal_type>
				<maxitems>1</maxitems>
				<minitems>0</minitems>
				<size>1</size>
				<type>group</type>
				<wizards>
				    <suggest>
					<type>suggest</type>
				    </suggest>
				</wizards>
			    </config>
			</TCEforms>
		    </settings.singlePid>
		</el>
	    </ROOT>
    	</General>

    </sheets>
</T3DataStructure>